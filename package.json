{
  "name": "prisma-shell",
  "version": "0.0.2",
  "description": "REPL for Prisma databases.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "access": "public",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangfisher/prisma-shell.git"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "build": "tsup",
    "changeset": "changeset",
    "release": "changeset version && changeset publish"
  },
  "bin": {
    "prisma-shell": "dist/cli.js"
  },
  "keywords": [
    "prisma",
    "repl",
    "shell",
    "tools"
  ],
  "author": "zhangfisher",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@prisma/client": "^6.0.1",
    "@types/node": "^22.10.2",
    "prisma": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "flex-tools": "^1.4.13",
    "logsets": "^1.3.14"
  },
  "peerDependencies": {
    "@prisma/client": "^6.0.1",
    "prisma": "^6.0.1"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "packageManager": "pnpm@9.0.0"
}
